                         PedidosProveedoresEntregas.Fecha) IS NOT NULL) AND (DATEDIFF(day, PedidosProveedoresMaestros.FechaServicio, PedidosProveedoresEntregas.Fecha) 
                         &lt;&gt; 0 AND DATEDIFF(day, PedidosProveedoresMaestros.FechaServicio, PedidosProveedoresEntregas.Fecha) &gt; 0) AND 
                         (PedidosProveedoresDetalles.FechaServicio &gt; CONVERT(DATETIME, '2013-01-01 00:00:00', 102))
