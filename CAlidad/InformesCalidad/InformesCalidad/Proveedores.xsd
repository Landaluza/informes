                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        PedidosProveedoresMaestros.Numero AS Referencia, Proveedores.Nombre AS Proveedor, CONVERT(varchar(10), PedidosProveedoresMaestros.FechaServicio, 103)
                          AS FechaSolicitadaMaestro, CONVERT(varchar(10), PedidosProveedoresEntregas.Fecha, 103) AS FechaEntrega, DATEDIFF(day, 
                         PedidosProveedoresMaestros.FechaServicio, PedidosProveedoresEntregas.Fecha) AS DiasEntrega
