		 INNER JOIN [dbo].[ControlIncidencias] ON [dbo].[ControlIncidencias_Proveedores].[Id_control_incidencias] = [dbo].[ControlIncidencias].[Id]
		 INNER JOIN [dbo].[Proveedores] ON [dbo].[ControlIncidencias_Proveedores].[id_proveedor] = [dbo].[Proveedores].[ProveedorID]
	WHERE
		 [dbo].[ControlIncidencias_Proveedores].[Id_control_incidencias] = @Id_control_incidencias
union
	SELECT
		  [dbo].[ControlIncidencias_Empleados].[Id]
		 ,[dbo].[Empleados].[Nombre] + ' ' + PrimerApellido + ' ' + SegundoApellido AS Nombre
		 ,'Empleado' Tipo
	FROM
		 [dbo].[ControlIncidencias_Empleados]
		 INNER JOIN [dbo].[ControlIncidencias] ON [dbo].[ControlIncidencias_Empleados].[Id_control_incidencias] = [dbo].[ControlIncidencias].[Id]
		 INNER JOIN [dbo].[Empleados] ON [dbo].[ControlIncidencias_Empleados].[Id_empleado] = [dbo].[Empleados].[Id]
	WHERE
		 [dbo].[ControlIncidencias_Empleados].[Id_control_incidencias] = @Id_control_incidencias     
) x
order by Nombre</CommandText>
