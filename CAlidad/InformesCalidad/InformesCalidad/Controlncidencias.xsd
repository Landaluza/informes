            <MainSource>
              <DbSource ConnectionRef="LAConnectionString (MySettings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        ControlIncidencias.Id, CONVERT(varchar(10), ControlIncidencias.Fecha, 103) AS Fecha, ControlIncidencias.NecesitaSeguimiento, ControlIncidencias.Observaciones, 
                         (TiposFrecuenciasIncidencias.Valor + TiposGravedadesIncidencias.Valores) * - 1 AS TotalGravedad, 
                         Empleados.Nombre + N' ' + Empleados.PrimerApellido + N' ' + Empleados.SegundoApellido AS Empleado, EstadosControlesIncidencias.Nombre, 
                         EstadosControlesIncidencias.Valor, DATEDIFF(day, ControlIncidencias.Fecha, { fn NOW() }) AS DiasAbierta
FROM            ControlIncidencias INNER JOIN
                         TiposFrecuenciasIncidencias ON ControlIncidencias.Id_PuntosFrecuencia = TiposFrecuenciasIncidencias.Id AND 
                         ControlIncidencias.Id_PuntosFrecuencia = TiposFrecuenciasIncidencias.Id AND ControlIncidencias.Id_PuntosFrecuencia = TiposFrecuenciasIncidencias.Id AND 
                         ControlIncidencias.Id_PuntosFrecuencia = TiposFrecuenciasIncidencias.Id INNER JOIN
                         TiposGravedadesIncidencias ON ControlIncidencias.Id_PuntosGravedad = TiposGravedadesIncidencias.Id AND 
                         ControlIncidencias.Id_PuntosGravedad = TiposGravedadesIncidencias.Id AND ControlIncidencias.Id_PuntosGravedad = TiposGravedadesIncidencias.Id AND 
                         ControlIncidencias.Id_PuntosGravedad = TiposGravedadesIncidencias.Id INNER JOIN
                         Empleados ON ControlIncidencias.id_empleado = Empleados.Id AND ControlIncidencias.id_empleado = Empleados.Id AND 
                         ControlIncidencias.id_empleado = Empleados.Id AND ControlIncidencias.id_empleado = Empleados.Id INNER JOIN
                         EstadosControlesIncidencias ON ControlIncidencias.EstadosControlesIncidencias = EstadosControlesIncidencias.Id
WHERE        (ControlIncidencias.NecesitaSeguimiento = 1)</CommandText>
